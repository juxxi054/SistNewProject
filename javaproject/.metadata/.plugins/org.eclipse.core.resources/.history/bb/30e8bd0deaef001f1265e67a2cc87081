package day2020;

public class Qize0220_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum=0;
		int tot=0;
		for(int i=1;i<=100;i++)
		{
			if(i%2==1)
				sum+=i;
			else
			tot+=i;
	}
		System.out.println("짝수의 합: "+sum);
		System.out.println("홀수의 합: "+tot);
	}
}