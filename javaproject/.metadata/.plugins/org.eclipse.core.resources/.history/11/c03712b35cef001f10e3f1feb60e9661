package day2020;

public class ForWhileReview_15 {
	
	public static void sumTest1() {
		//for문으로 1~5까지 구하기
		int sum=0
	    for(int i=1;i<=5;i++)
	    {
	    	sum+=i; // sum=sum+1
	    }
		System.out.println("총합계는"+sum+"입니다");
	}

