package day0217;

public class QuizwArgs {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("이름:"+args[0]);
		System.out.println("나이:"+args[1]+"세");
		System.out.println("지역:"+args[2]+"시");
	System.out.println("나는"+args[2]+"에 사는"+args[1]+args[0]+"입니다");

        System.out.println("이름:"+args[0]);
        System.out.println("나이:"+args[1]);
        System.out.println("지역:"+args[2]);
	}

}
