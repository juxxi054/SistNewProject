package day0228;



class coffe{
	
	private String americano;
	private String tea;
	private static String  mes="Happy Day";
	
	
	public String getAmericano() {
		return americano;
	}
	public void setAmericano(String americano) {
		this.americano = americano;
	}
	public String getTea() {
		return tea;
	}
	public void setTea(String tea) {
		this.tea = tea;
	}
	public static String getMes() {
		return mes;
	}
	public static void setMes(String mes) {
		coffe.mes = mes;
	}
	
	

}
public class Ex10Object {

	//coffee클래스의 변수값을 출력하기 위한 메서드
	public static void writeCoffe()
	{
		System.out.println("cofee클래스 출력");
		System.out.println("커피종류:"+coffe.getAmericano());
		System.out.println("커피종류:"+coffe.getTea());
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	System.out.println("커피클래스의 메세지 출력");
	System.out.println(coffee.getMes());
		
	}
	
	
	

}
