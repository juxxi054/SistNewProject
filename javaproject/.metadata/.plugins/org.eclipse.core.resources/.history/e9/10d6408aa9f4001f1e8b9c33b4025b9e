package day0227;

public class Ex3WorkerMethod {

	class MyInfo{
	
	private String name;	
	private int age;

	
	//setter 메서드
	public void setName(String name)
	{
	   this.name=name;	
	}
	
	public void setAge(int age)
	{
		this.age=age;
	}
	
	
	//getter 메서드
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	MyInfo my1=new MyInfo();
	my1.setName("주히");
	my1.setAge(25);
	
	
		
		
	}

}
