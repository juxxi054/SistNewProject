package day2021;

import java.util.Scanner;

public class AA {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=0,b=0;
		int num;
		int su=0;
		
		for(int i=0;i<=5;i++);
		{
		num=sc.nextInt();
		System.out.println(num+":");
		}
		
		if(su>0)
		  a+=1;
		else if (su<0)
		  b+=1;
		System.out.println("양수갯수"+a);
		System.out.println("음수갯수"+a);
			}
		

	}


