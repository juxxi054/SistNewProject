		 * 5번 점수:-77
		 *        잘못입력했어요
		 *=================
		 *총점:****  
		 */
           
		
		Scanner sc=new Scanner(System.in);
		
		int sum;
		int score;
		
		for(int i=0;i<=5;i++);
		{
		 System.out.print(i+"번째 점수를 입력하세요");
		}
		
	}

