package day2020;

public class ForTest_11 {

	public static void main(String[] args) {
		// 1~10까지 숫자 가로로 출력

		
		for(int i=1;i<=10;i++)
			System.out.print(i+" ");
		System.out.println();
		
		// 10~1 까지 숫자 가로로 출력
		
		for(int i=10;i>=1;i--)
			System.out.print(i+" ");
		System.out.println();
		
		//Hello1 Hello2 Hello3 출력
		
		for(int i=1;i<=3;i++)
			System.out.print("Hello"+i+" ");
		System.out.println();
		
		//1,4,7....50까지 3의 배수 구하기
		for(int i=1;i<=50;i+=3)
			System.out.print(i+" ");
		System.out.println();
	}

}
