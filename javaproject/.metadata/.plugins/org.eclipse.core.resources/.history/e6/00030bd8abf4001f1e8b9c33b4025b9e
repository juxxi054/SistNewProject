package day0226;

public class QuizObject {
 
	
	static String processName; //과정명
	String stuName; //학생명
    int age; //학생들의 나이
    private String city; //사는곳
    private String hobby;
 
    public Student(String stuName, int age, String city, String hobby)
	   {
	        this.stuName = stuName;
	        this.age = age;
	        this.city = city;
	        this.hobby = hobby;
	        
	public void displayStudentInfo() {
     System.out.println("이름: " + stuName);
     System.out.println("나이: " + age);
     System.out.println("지역: " + city);
     System.out.println("취미: " + hobby);
     System.out.println(); // 구분을 위한 빈 줄
    }
}

