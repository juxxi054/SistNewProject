package day2020;

public class ForWhlieTest_09 {

	public static void main(String[] args) {
		// 10 9 8 7......0
		for(int i=10;i>=0;i--)
		{
          System.out.print(i+" ");
	}
       //while
		int a=10;
		
		while(a>=0)
		{
			  System.out.print(a--+" ");//초기값을 출력후 하나 감소
		}
		
		// 5 4 3 2 1을 for문으로 출력해보세요
		
		for(int b=5;b>=1:b--)
		{
		  System.out.print(b+" ");
		}
}
}