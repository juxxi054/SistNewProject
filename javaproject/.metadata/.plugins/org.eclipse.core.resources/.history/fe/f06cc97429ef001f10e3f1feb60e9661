package day2020;

public class SwichTest_02 {

	public static void main(String[] args) {
		//switch이용예제
		
		int n=1;
		
		switch(n)
		{
		case 1:
			System.out.println("n is 1");
		case 2:
			System.out.println("n is 2");
		case 3:
			System.out.println("n is 3");
		default:
			System.out.println("n은 3보다 크다");
		}

	}

}
