package day0304;

class shop{
	
	private String sangpum;
	private int su;
	
	//디폴트 생성자(상품명=딸기,수량=50)
	
	public shop(){
		this.sangpum="딸기";
		this.su=50;
	}
	//명시적 생성자(상품명,수량)
	public shop(String sang,int su){
		this.sangpum=sang;
		this.su=su;
	}
	
	//출력 writeShop(상품명:초코파이 수량:100개)
	public void writeShop() {
		System.out.println("상품명:"+sangpum);
		System.out.println("수량"+su);
	}
}

////////shop을 상속받는다,생성자 2개와 메소드 하나 완성 
class Myget extends shop{
	private int price;
	
	public  Myget() {
		price=2500;
	}
	public Myget(String sang;int su,int price) {
		super("초코파이",25);
		this.price=price;
	}

}
////////// 2개의 객체를 생성후 출력 1.디폴트생성자 2.인자있는 생성자
